# Docker setup
COMPOSE_FILE=./docker/docker-compose.yml

# Gatsby setup
NODE_ENV=development
APP_PORT=3000
APP_HOST=0.0.0.0
INTERNAL_STATUS_PORT=8080
CHOKIDAR_USEPOLLING=1
GATSBY_WEBPACK_PUBLICPATH=/
GATSBY_GRAPHQL_IDE=playground
ENABLE_GATSBY_REFRESH_ENDPOINT=true
GATSBY_REFRESH_TOKEN=12345
SHARP_IGNORE_GLOBAL_LIBVIPS=1

# App setup
APP_TITLE=Michelle's Portfolio
APP_DESC=A site full of Michelle Chai's projects.
APP_AUTHOR=Michelle Chai
APP_SITE_URL=https://michellemtchai.github.io/

# Contentful setup
CONTENTFUL_SPACE_ID=placeholder
CONTENTFUL_ACCESS_TOKEN=placeholder
